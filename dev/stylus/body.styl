header
	width 16rem
	height 2.25rem
	background rgb(250,250,250) url("../../image/headbg.png")  7rem no-repeat
	background-size 7rem 2.2rem
	a
		display inline-block
		padding 0.35rem 0 0 0.5rem
	.logo
		width 6rem
		height 1.6rem
	.more
		float right
		padding 0.75rem 0.75rem 0 0
.hidden
	width 16rem
	height 5.2rem
	&.top
		display none
		background rgb(73,152,231)
		border-top 0.2rem solid rgb(49,126,203)
		a
			font-size 0.8rem
			color white
	&.foot
		background rgb(73,83,93)
		a
			font-size 0.7rem
			color #95a0ac
	.channels
		display block
		padding 5px 0
		text-align center
		a
			display inline-block
			width 2.8rem
			height 1.5rem
			line-height 1.5rem
			
			
.nav
	width 16rem
	height 2.1rem
	padding 0.25rem 0
	background rgb(62,134,206)
	a
		padding  0.375rem 0.825rem 
		color white
		font-size 0.8rem
		line-height 1.6rem
		&.current
			background rgb(34,111,187)
			text-align center
			line-height 1.5rem
.gallery
	width 16rem
	position relative
	img
		width 16rem
		height 8.25rem
	.desc
		position absolute
		left 0
		bottom 0
		right 0
		color white
		font-size 0.8rem
		padding 0 1.8rem 0 0
		height 1.6rem
		line-height 1.6rem
		padding-left 0.6rem
		text-shadow 1px 1px 1px black
		background -webkit-linear-gradient(top,rgba(0,0,0,0),rgba(0,0,0,.9))
.aoyun
	width 16rem
	padding 0.25rem 0.5rem
	line-height 1.5rem
	.gold
		border 1px solid #CDDEF0
		padding-bottom 0.1rem
		margin-bottom 0.2rem
		.gold-hd
			height 1.25rem
			background url("../../image/jpb_hd.png") no-repeat
			background-size cover
			font-size 0.7rem
			line-height 1.25rem
			color #006ace
			padding 0 0.3rem
			strong
				font-weight bold
		.gold-bd
			position relative
			height 1.25rem
			overflow hidden
			ul
				position absolute
				top 0
				width 15.2rem
				height 1.7rem
				opacity 0
				&.first
					opacity 1
				&.second1
					opacity 1
					animation second 0.5s forwards
				&.third1
					opacity 1
					animation third 0.5s forwards
				li
					float left
					height .8rem
					line-height .8rem
					margin .45rem 0
					font-size .7rem
					span
						display inline-block
						color white
						padding	.05rem .2rem
						border-radius .1rem
						&.one
							background-color rgb(255,131,39)
						&.two
							background-color rgb(96,187,70)
					&.pai
						position relative
						&:before
							position absolute
							left .6rem
							top 0
							display block
							content ""
							width .7rem
							height .7rem
							background-image url(../../image/rank_icons2.png)
							background-size 5.8rem auto
							background-repeat no-repeat
					&.icon
						width 2rem
						margin-left .2rem
						text-align center
					&.country
						width 4.1rem
						text-align center
					&.goldNum
						width 2.2rem
						padding-left 1.4rem
					&.silverNum
						width 2.2rem
						padding-left 1.4rem
						&:before
							background-position -2.7rem 0
					&.bronzeNum
						width 2.2rem
						padding-left 1.4rem
						&:before
							background-position -5.1rem 0
					&.totalNum
						width 2.2rem
						text-align center
	.ad
		display block
		height 1.7rem
		line-height 1.7rem
		font-size 0.7rem
		border .05rem solid #CDDEF0
		text-align center
		background #EDFAFF
		overflow hidden
		color #0695d7
		img
			display inline
			height 1.5rem
			margin-right 0.25rem
			vertical-align middle
		strong
			font-weight bold
.list
	padding 0 0.4rem
	ul
		li
			overflow hidden
			border-bottom .05rem solid #eceef0
			box-shadow 0 .05rem .05rem #fff
		.total
			padding .5rem 0
			a
				display block
				img
					width 3.75rem
					height 2.75rem
					line-height 2.9rem
					padding 0.05rem
					background #e9f1f4
					border 0.05rem solid #e0e0e0
					float left
					text-align center
				h2
					height 1.1rem
					line-height 1.1rem
					overflow hidden
					font-size 0.76rem
					padding-left 0.25rem
					margin-bottom 0.2rem
					white-space nowrap
					text-overflow ellipsis
				p
					padding 0 0 0 0.25rem
					line-height 0.8rem
					font-size 0.66rem
					overflow hidden
					max-height 1.7rem
					color #878c92
					.count
						height 0.85rem
						line-height 0.85rem
						display inline-block
						text-align center
						float right
						i
							display inline-block
							font-size 0.68rem
							color #878c92
							&.flag
								vertical-align middle
								margin-right 0.2rem
								width 1.2rem
								height 0.7rem
								background-image url("../../image/flagicon.png")
								background-repeat no-repeat
								background-size 8.4rem 0.7rem
								&.flag4
									background-position -3.6rem 0
							&.comm
								margin-left 0.3rem
								&:before
									width 0.54rem
									height 0.54rem
									display inline-block
									background-position -4.16rem 0
									margin-right 0.15rem
									background-image url("../../image/icon.png")
									background-repeat no-repeat
									background-size 5.95rem 1.05rem
									vertical-align middle
									content ""
.more
	height 1.65rem
	line-height 1.65rem
	margin 0 0.4rem 0.75rem
	border 0.05rem solid #efefef
	border-color #f6f5f5
	color #b4b4b4
	text-align center
	background #f6f5f5
	.getmore:after
		width 1.05rem
		height 1.05rem
		display inline-block
		vertical-align -5px
		margin-left 0.25rem
		background-position -2.1rem 0
		background-image url("../../image/icon.png")
		background-repeat no-repeat
		background-size 5.95rem 1.05rem
		content ""
.footer
	padding 0.5rem 2.25rem 0.6rem 0 
	overflow hidden
	position relative
	background #404a54
	.footer-user
		color #95a0ac
		font-size 0.7rem
		line-height 0.7rem
		padding 0 0.4rem
	.links
		height 1.25rem
		line-height 1.25rem
		color #565e61
		font-size 0.7rem
		text-align left
		a
			color #95a0ac
			margin 0 0 0 0.4rem
			display inline
			float left
		.pc:before
			background #565e61
			width 0.05rem
			height 0.6rem
			display inline-block
			color #565e61
			margin-right 0.4rem
			content ""
	.copyright
		font-size 0.7rem
		color #707982
		text-align left
		text-indent 0.4rem
		white-space nowrap
	.gotop
		position absolute
		right 0.6rem
		top 0.5rem
		background #313a43
		width 2rem
		height 1.8rem
		&:after
			display block
			margin 0.5rem auto 0
			width 1.24rem
			height 0.75rem
			background-position -4.75rem 0
			background-image url("../../image/icon.png")
			background-repeat no-repeat
			background-size 5.95rem 1.05rem
			content ""
.today
	display none
	position fixed
	left 0
	top 0
	width 100%
	height 100%
	z-index 1001
	background-color rgba(0,0,0, .2)
	.today-inner
		display none
		width 13.75rem
		height 19.75rem
		position fixed
		left 6%
		bottom 20%
		background url("../../image/today-inner.png") center bottom no-repeat
		background-size cover
		
		z-index 999
		.close
			position absolute
			right 10%
			top 10%
			width 1.5rem
			height 1.5rem
			background-image url("../../image/close.png")
			background-size 100%
		.enter
			position absolute
			left 0
			bottom 0
			width 100%
			height 50%
			background-image url("../../image/enter.png") no-repeat
			background-size 100%
.today-btn
	position fixed
	right 0.5rem
	bottom 4rem
	width 2.4rem
	height 2.8rem
	background-image url("../../image/today-s.png")
	background-size cover
	z-index 999

@keyframes second
	from
		top 0rem
	to
		top -1.7rem	
@keyframes third
	from
		top 1.7rem
	to
		top 0rem

	